{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://az-use-case-perf-tests.uc-non-persistent.com/consumer.json",
    "type": "object",
    "required": [
      "consumer_id", "topic_list"
    ],
    "properties": {
      "consumer_id": {
        "$ref": "file:./schemas/schema.id.json"
      },
      "topic_list": {
        "type": "string",
        "pattern": "^[-a-zA-Z0-9_/>,\\*\\?]+$"
      }
    },
    "additionalProperties": false
}
